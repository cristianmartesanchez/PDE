@model PDE.Models.Entities.Miembro
        
<form asp-action="Create">            
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="row">
                <div class="col-md-6 form-group">
                    <label asp-for="Nombres" class="control-label"></label>
                    <input asp-for="Nombres" readonly="true" class="form-control" />
                    <span asp-validation-for="Nombres" class="text-danger"></span>
                </div>
                <div class="col-md-6 form-group">
                    <label asp-for="Apellidos" class="control-label"></label>
                    <input asp-for="Apellidos" readonly="true" class="form-control" />
                    <span asp-validation-for="Apellidos" class="text-danger"></span>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 form-group">
                    <label asp-for="FechaNacimiento" class="control-label"></label>
                    <input asp-for="FechaNacimiento" readonly="true"  class="form-control" />
                    <span asp-validation-for="FechaNacimiento" class="text-danger"></span>
                </div>
                <div class="col-md-6 form-group">
                    <label asp-for="LugarNacimiento" class="control-label"></label>
                    <input asp-for="LugarNacimiento" readonly="true" class="form-control" />
                    <span asp-validation-for="LugarNacimiento" class="text-danger"></span>
                </div>
            </div>

             <div class="row">
               <div class="col-md-6 form-group">
                    <label asp-for="SexoId" class="control-label">Sexo</label>
                    <input asp-for="SexoId" type="hidden" class="form-control" />
                    <input value="@Model.Sexo.Descripcion" class="form-control" readonly="readonly" />
                </div>
               <div class="col-md-6 form-group">
                    <label asp-for="EstadoCivilId" class="control-label">Estado Civil</label>
                    <input asp-for="EstadoCivilId" type="hidden" class="form-control" />
                    <input value="@Model.EstadoCivil.Descripcion" class="form-control" readonly="readonly" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 form-group">
                    <label asp-for="Cedula" class="control-label"></label>
                    <input asp-for="Cedula" readonly="true"  class="form-control" />
                    <span asp-validation-for="Cedula" class="text-danger"></span>
                </div>
                <div class="col-md-6 form-group">
                    <label asp-for="NacionalidadId" class="control-label">Nacionalidad</label>
                    <input asp-for="NacionalidadId" type="hidden" class="form-control" />
                    <input value="@Model.Nacionalidad.Descripcion" class="form-control" readonly="readonly" />
                </div>
            </div>
              <div class="row">
                <div class="col-md-6 form-group">
                    <label asp-for="OcupacionId" class="control-label">Ocupación</label>
                    <input value="@Model.Ocupacion.Descripcion" readonly="readonly"  class="form-control" />
                    <input asp-for="OcupacionId" type="hidden" class="form-control" />
                </div>
                <div class="col-md-6 form-group">
                    <label asp-for="CategoriaId" class="control-label">Categoria</label>
                    <input asp-for="CategoriaId" type="hidden" class="form-control" />
                    <input value="@Model.Categoria.Descripcion" class="form-control" readonly="readonly" />
                </div>
            </div>
            <div class="row">
                 <div class="col-md-6 form-group">
                    <label asp-for="MunicipioId" class="control-label">Municipio</label>
                    <input asp-for="MunicipioId" type="hidden" class="form-control" />
                    <input id="ProvinciaId" value="@Model.Municipio.ProvinciaId" type="hidden" class="form-control" />
                    <input value="@Model.Municipio.Descripcion" class="form-control" readonly="readonly" />
                </div>
                <div class="col-md-6 form-group">
                    <label  class="control-label">Provincia</label>
                    <input value="@Model.Municipio.Provincia.Descripcion" class="form-control" readonly="readonly" />                    
                </div>
            </div>
             <div class="row">
                 <div class="col-md-6 form-group">
                    <label asp-for="ColegioId" class="control-label">Colegio</label>
                    <input asp-for="ColegioId" type="hidden" class="form-control" />
                    <input value="@Model.Colegio.Descripcion" class="form-control" readonly="readonly" />
                </div>
                <div class="col-md-6 form-group">
                    <label  class="control-label">Recinto</label>
                    <input value="@Model.Colegio.Recinto.CodigoRecinto" class="form-control" readonly="readonly" />                    
                </div>
            </div>
            <div class="row">
                 <div class="col-md-6 form-group">
                    <label asp-for="Celular" class="control-label"></label>
                    <input asp-for="Celular" class="form-control" autocomplete="off" />
                    <span asp-validation-for="Celular" class="text-danger"></span>
                </div>
                <div class="col-md-6 form-group">
                    <label asp-for="LocalidadId" class="control-label">Localidad</label>
                    <select asp-for="LocalidadId" asp-items="@ViewBag.Localidad" class="form-control">
                        <option >--Seleccione una localidad--</option>
                    </select>
                </div>
            </div>
            <div class="row">
                 <div class="col-md-6 form-group">
                    <label asp-for="CargoId" class="control-label">Cargo Territorial</label>                    
                    <select asp-for="CargoId" class="form-control"></select>
                    <span asp-validation-for="CargoId" class="text-danger"></span>
                </div>
                <div class="col-md-6 form-group">
                    <label asp-for="SupervisorId" class="control-label">Supervisor</label>                    
                    <select asp-for="SupervisorId" class="form-control"></select>
                    <span asp-validation-for="SupervisorId" class="text-danger"></span>
                </div>
            </div>
            <hr />
            <div class="row">
                <div class="col-md-12 form-group">
                    <input type="submit" value="Guardar" class="btn btn-primary float-sm-end" />
                </div>
            </div>

        </form>